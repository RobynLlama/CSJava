{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Build Scripts",
      "type": "shell",
      "command": "npm",
      "args": [
        "run",
        "build"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "presentation": {
        "reveal": "always",
        "panel": "shared"
      },
      "problemMatcher": [
        "$tsc"
      ]
    }
  ]
}
